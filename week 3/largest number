def large(li):
	big=li[0]
	for i in range(1,len(li)):
		if li[i]>big:
			big=li[i]
	return big
def print_list(li):
	for i in li:
		print(i,end=' ')
elements=[]
limit=int(input("enter limit for list:"))
for i in range(limit):
	val=int(input("enter value:"))
	elements.append(val)
print("elements in the list are")
print_list(elements)
print()
print("the largest element is=",large(elements))
